{
  "manifest_version": 3,
  "name": "NUSENSE TryON",
  "version": "2.0",
  "description": "Transformez votre style avec l'essayage virtuel alimenté par IA. Sélectionnez des vêtements de n'importe quel site web et voyez comment ils vous vont instantanément.",
  "permissions": ["activeTab", "scripting", "storage"],
  "host_permissions": [
    "https://try-on-server-v1-294135365335.europe-west9.run.app/*",
    "https://accounts.google.com/*",
    "https://www.googleapis.com/*",
    "https://*/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/16.png",
      "20": "icons/20.png",
      "29": "icons/29.png",
      "32": "icons/32.png",
      "40": "icons/40.png",
      "48": "icons/48.png",
      "50": "icons/50.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icons/16.png",
    "20": "icons/20.png",
    "29": "icons/29.png",
    "32": "icons/32.png",
    "40": "icons/40.png",
    "48": "icons/48.png",
    "50": "icons/50.png"
  }
}
